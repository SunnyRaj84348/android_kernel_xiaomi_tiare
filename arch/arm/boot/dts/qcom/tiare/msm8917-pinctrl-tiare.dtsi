&soc {
	tlmm: pinctrl@1000000 {
		smb_int_pin {
			//qcom,pins = <&gp 61>;
			pins = "gpio61";
			qcom,num-grp-pins = <1>;
			qcom,pin-func = <0>;
			label = "smb_int_gpio";
			smb_int_default: smb_int_default {
				drive-strength = <2>;	/*8 MA */
				bias-pull-up;		/* PULL UP*/
			};
		};
		bq_int_default: bq_int_default {
			mux {
				pins = "gpio25";
				function ="gpio";
			};
			config {
				pins = "gpio25";
				drive-strength = <2>; /* 2 MA */
				bias-pull-up;
				/* PULL UP*/
				};
		};

		gpio_led_pins {
			gpio_led_off: gpio_led_off {
				mux {
					pins = "gpio39";
					function = "gpio";
				};
				config {
					pins = "gpio39";
					drive-strength = <6>;
					bias-pull-down;
					output-low;
				};
			};
		};
		i2c_2 {
			i2c_2_sleep: i2c_2_sleep {
				config {
					/delete-property/ bias-disable;
					bias-pull-down;
				};
			};
		};
	};
};
